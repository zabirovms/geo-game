externalPort = 3001

[[ports]]
localPort = 37165
externalPort = 3002

[[ports]]
localPort = 46591
externalPort = 3000

[[ports]]
localPort = 46873
externalPort = 3003

[deployment]
deploymentTarget = "autoscale"
run = ["npx", "serve", "-s", "build", "-l", "5000"]
build = ["npm", "run", "build"]
